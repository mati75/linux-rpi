+CONFIG_TCG_TPM=m
+CONFIG_TCG_TIS_SPI=m
 CONFIG_I2C=y
 CONFIG_I2C_CHARDEV=m
 CONFIG_I2C_MUX=m
@@ -1343,12 +1344,12 @@ CONFIG_NLS_ISO8859_15=m
 CONFIG_NLS_KOI8_R=m
